<h1>New timesheet</h1>

<% form_for(@timesheet) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :log_date %><br />
    <%= f.datetime_select :log_date %>
  </p>
  <p>
    <%= f.label :hours_spent %><br />
    <%= f.text_field :hours_spent %>
  </p>
  <p>
    <%= f.label :employee_id %><br />
    <%= f.text_field :employee_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', timesheets_path %>